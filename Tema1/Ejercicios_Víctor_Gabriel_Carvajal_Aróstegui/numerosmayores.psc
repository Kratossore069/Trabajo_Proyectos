Algoritmo arrays
	
	// Insertar 5 datos al usuario. Hecho esto, mostramos la distancia más pequeña
	// de todos esos números respecto a la primera.
	
	Escribir "4 números";
	Leer num1, num2, num3, num4;
	
	si num1>num2
		comodin = num1;
	SiNo
		comodin=num2;
	FinSi
	
	si comodin>num3
		comodin=comodin;
	SiNo
		comodin=num3;
	FinSi
	
	si comodin>num4
		comodin=comodin;
	SiNo
		comodin=num4;
	FinSi
	
	Escribir "Mayor es",comodin;
	
	
FinAlgoritmo
