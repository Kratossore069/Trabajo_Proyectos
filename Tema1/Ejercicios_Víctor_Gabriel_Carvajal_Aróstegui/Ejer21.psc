Algoritmo numerosporelusuario
	
	// Sacar potencias de 2 entre dos números dados por el usuario.
	Escribir "Primer número: ";
	Leer n;
	Escribir "Segundo número: ";
	Leer m;
	
	si n<m
		minimo = n;
		maximo = m;
	SiNo
		minimo = m;
		maximo = n;
	FinSi
	
	Escribir "El mínimo es: ", minimo;
	Escribir "El máximo es: ", maximo;
	
	
FinAlgoritmo
